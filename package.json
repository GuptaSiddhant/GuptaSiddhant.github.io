{
  "private": true,
  "name": "guptasiddhant-com",
  "description": "Personal website of Siddhant Gupta, build with Remix.",
  "license": "",
  "scripts": {
    "build": "npm run build:css && npm run build:remix",
    "build:remix": "remix build",
    "build:css": "tailwindcss -i ./tailwind.css -o ./app/styles/tailwind.css --minify",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:remix\"",
    "dev:remix": "node -r dotenv/config node_modules/.bin/remix dev",
    "dev:css": "tailwindcss -i ./tailwind.css -o ./app/styles/tailwind.css --watch",
    "postinstall": "remix setup node",
    "clear": "rm -rf api/build; rm -rf public/build; rm -rf .cache; rm app/styles/tailwind.css"
  },
  "dependencies": {
    "@octokit/plugin-throttling": "^3.5.2",
    "@octokit/rest": "^18.12.0",
    "@reach/tooltip": "^0.16.2",
    "@react-aria/focus": "^3.5.0",
    "@react-aria/label": "^3.2.1",
    "@react-aria/ssr": "^3.1.0",
    "@remix-run/react": "1",
    "@remix-run/serve": "1",
    "@remix-run/vercel": "1",
    "clsx": "^1.1.1",
    "dotenv": "^10.0.0",
    "mdx-bundler": "^8.0.1",
    "octokit": "^1.7.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-text-transition": "^1.3.0",
    "remix": "^1.1.0",
    "remixicon-react": "^1.0.0",
    "use-copy": "^0.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/tailwindcss": "^2.2.4",
    "concurrently": "^6.4.0",
    "eslint": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "prettier": "^2.5.0",
    "prettier-plugin-groq": "^0.2.5",
    "tailwindcss": "^3.0.0",
    "typescript": "^4.5.0"
  },
  "optionalDependencies": {
    "esbuild-linux-64": "*"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
