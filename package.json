{
  "private": true,
  "name": "guptasiddhant-com",
  "description": "Personal website of Siddhant Gupta, build with Remix.",
  "license": "",
  "main": "index.js",
  "scripts": {
    "build": "remix build",
    "build:css": "tailwindcss -i ./tailwind.css -o ./app/styles/tailwind.css",
    "clear": "rimraf .cache/ .firebase/ build/ public/build/ *.log app/tailwind.css",
    "deploy": "firebase deploy",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:remix\" \"npm run start\"",
    "dev:css": "tailwindcss -i ./tailwind.css -o ./app/styles/tailwind.css --watch",
    "dev:remix": "remix watch",
    "postinstall": "remix setup node",
    "prebuild": "npm run build:css",
    "start": "firebase emulators:start"
  },
  "dependencies": {
    "@netlify/functions": "^0.10.0",
    "@octokit/core": "^3.5.1",
    "@octokit/plugin-throttling": "^3.5.2",
    "@octokit/rest": "^18.12.0",
    "@reach/dialog": "^0.16.2",
    "@reach/menu-button": "^0.16.2",
    "@reach/tooltip": "^0.16.2",
    "@reach/window-size": "^0.16.0",
    "@react-aria/focus": "^3.5.0",
    "@react-aria/label": "^3.2.1",
    "@react-aria/ssr": "^3.1.0",
    "@remix-run/react": "1",
    "clsx": "^1.1.1",
    "copy-to-clipboard": "^3.3.1",
    "mdx-bundler": "^8.0.1",
    "octokit": "^1.7.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-text-transition": "^1.3.0",
    "remix": "^1.1.0",
    "remixicon-react": "^1.0.0"
  },
  "devDependencies": {
    "@remix-run/node": "1",
    "@remix-run/dev": "1",
    "@remix-run/express": "1",
    "compression": "1.7.4",
    "express": "^4.17.1",
    "firebase-admin": "^10.0.1",
    "firebase-functions": "^3.14.1",
    "morgan": "1.10.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/tailwindcss": "^2.2.4",
    "@vercel/node": "^1.12.1",
    "concurrently": "^6.4.0",
    "eslint": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.0",
    "typescript": "^4.5.0"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
